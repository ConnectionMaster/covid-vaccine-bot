{
	"name": "ms-covid-healthbot-service",
	"version": "0.0.2",
	"description": "",
	"scripts": {
		"build:tsc": "tsc",
		"build:docker": "yarn node ./scripts/build.js --imageNameSpace ${AZ_CONTAINER_REGISTRY} --skipVersionTag --verbose",
		"bundle": "yarn node ./scripts/build.js --imageNameSpace ${AZ_CONTAINER_REGISTRY} --verbose",
		"clean": "rimraf dist",
		"start:tsc": "tsc -w",
		"start:func": "func start",
		"start:docker": "docker run --rm -p 7071:80 ${AZ_CONTAINER_REGISTRY}/$(node -p 'require(\"./package.json\").name')",
		"deploy": "yarn node ./scripts/deploy.js --containerRegistry ${AZ_CONTAINER_REGISTRY} -u ${DOCKER_USER} -p ${DOCKER_PASSWORD} --verbose",
		"start": "run-p start:tsc start:func"
	},
	"devDependencies": {
		"@azure/functions": "^1.2.2",
		"@types/debug": "^4",
		"@types/minimist": "^1",
		"@types/node": "^14.14.22",
		"@types/node-fetch": "^2",
		"@types/rimraf": "^3",
		"azure-functions-core-tools": "^3.0.3233",
		"debug": "^4.3.1",
		"execa": "^5.0.0",
		"minimist": "^1.2.5",
		"minimist-options": "^4.1.0",
		"npm-run-all": "^4.1.5",
		"rimraf": "^3.0.2",
		"typescript": "^4.1.3"
	},
	"dependencies": {
		"dotenv": "^8.2.0",
		"node-fetch": "^2.6.1"
	}
}
