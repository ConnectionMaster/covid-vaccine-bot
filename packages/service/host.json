{
	"version": "2.0",
	"logging": {
		"applicationInsights": {
			"samplingSettings": {
				"isEnabled": true,
				"excludedTypes": "Request;Exception"
			}
		},
		"fileLoggingMode": "always",
		"logLevel": {
			"default": "Information",
			"Host": "Error",
			"Function": "Information"
		},
		"console": {
			"isEnabled": true
		}
	},
	"extensionBundle": {
		"id": "Microsoft.Azure.Functions.ExtensionBundle",
		"version": "[1.*, 2.0.0)"
	}
}
