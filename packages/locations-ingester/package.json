{
	"name": "@c19-vax-api/locations-ingester",
	"description": "VaccineFinder data ingestion scripts",
	"private": true,
	"version": "1.0.0",
	"main": "index.js",
	"license": "MIT",
	"scripts": {
		"build": "tsc -b .",
		"prestart": "npm run build && func extensions install",
		"ingest:fetch": "ts-node src/fetchS3Data",
		"ingest:xform": "ts-node src/transformData",
		"ingest:geocode": "ts-node src/geocodeData",
		"ingest:write": "ts-node src/writeCosmosData",
		"start:ingest": "run-s ingest:fetch ingest:xform ingest:geocode ingest:write",
		"start:fn": "func start"
	},
	"dependencies": {
		"@azure/cosmos": "^3.10.5",
		"aws-sdk": "^2.880.0",
		"axios": "^0.21.1",
		"batch2": "^2.0.0",
		"config": "^3.3.6",
		"csv-parse": "^4.15.3",
		"lodash": "^4.17.21",
		"progress": "^2.0.3"
	},
	"devDependencies": {
		"@azure/functions": "^1.0.1-beta1",
		"@tsconfig/node14": "^1.0.0",
		"@types/config": "^0.0.38",
		"@types/lodash": "^4",
		"@types/node": "^14.14.37",
		"@types/progress": "^2",
		"npm-run-all": "^4.1.5",
		"ts-node": "^9.1.1",
		"typescript": "^4.2.3"
	}
}
