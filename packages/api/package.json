{
	"name": "@covid-vax-bot/api",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"start": "nodemon src/index.ts",
		"start:api": "yarn start",
		"compile": "tsc -b .",
		"writeDeployPackageJson": "node writeDeployPackageJson",
		"copyConfiguration": "shx cp -r config/ dist/",
		"copyLib": "shx cp -r lib/ dist/",
		"build": "run-s compile writeDeployPackageJson copyConfiguration copyLib"
	},
	"dependencies": {
		"@azure/cosmos": "^3.11.0",
		"axios": "^0.21.1",
		"config": "^3.3.6",
		"cors": "^2.8.5",
		"express": "^4.17.1",
		"express-openapi": "^7.5.0",
		"geodist": "^0.2.1"
	},
	"devDependencies": {
		"@tsconfig/node14": "^1.0.0",
		"@types/body-parser": "^1.19.0",
		"@types/config": "^0.0.38",
		"@types/cors": "^2.8.10",
		"@types/express": "^4.17.11",
		"@types/node": "^14.14.43",
		"body-parser": "^1.19.0",
		"nodemon": "^2.0.7",
		"npm-run-all": "^4.1.5",
		"openapi-types": "^7.2.3",
		"shx": "^0.3.3",
		"ts-node": "^9.1.1",
		"typescript": "^4.2.4"
	}
}
