{
	"name": "@covid-vax-bot/plans",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"clean": "essex clean",
		"assemble_localization": "ts-node scripts/assembleLocalization",
		"assemble_policies": "ts-node scripts/assemblePolicies",
		"assemble_sitelist": "ts-node scripts/aggregateInfoSites",
		"generate_review_document": "ts-node scripts/generateReviewDocument",
		"test": "ts-node scripts/validatePolicies",
		"build": "run-s assemble_localization assemble_policies assemble_sitelist generate_review_document checksum",
		"deploy_resources": "ts-node scripts/healthbot/deployResources",
		"deploy_localizations": "ts-node scripts/healthbot/deployLocalizations",
		"deploy": "run-s checksum deploy_localizations deploy_resources",
		"deploy:data": "yarn deploy",
		"checksum:data": "checksum dist/policies.json",
		"checksum:localization": "checksum dist/localization.csv",
		"checksum": "run-p 'checksum:*'",
		"localize": "ts-node scripts/localize es es-us",
		"clearLocalization": "ts-node scripts/clearLocalization es-us"
	},
	"devDependencies": {
		"@azure/storage-blob": "^12.5.0",
		"@covid-vax-bot/plan-schema": "workspace:packages/schema",
		"@essex/scripts": "^16.0.4",
		"@types/checksum": "^0.1.31",
		"@types/config": "^0.0.38",
		"@types/jsonwebtoken": "^8.5.1",
		"@types/node": "^14.14.43",
		"@types/uuid": "^8.3.0",
		"ajv": "^7.2.4",
		"axios": "^0.21.1",
		"chalk": "^4.1.1",
		"checksum": "^0.1.1",
		"config": "^3.3.6",
		"csv-parse": "^4.15.4",
		"csv-stringify": "^5.6.2",
		"dotenv": "^8.2.0",
		"form-data": "^3.0.1",
		"jsonwebtoken": "^8.5.1",
		"moment": "^2.29.1",
		"npm-run-all": "^4.1.5",
		"ts-node": "^9.1.1",
		"typescript": "^4.2.4",
		"typescript-json-schema": "^0.50.0",
		"uuid": "^8.3.2"
	}
}
