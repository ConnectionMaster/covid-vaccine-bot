{
	"name": "@covid-vax-bot/healthbot-app",
	"version": "0.0.1",
	"description": "A simple web page to handoff users to the Health bot",
	"main": "server.js",
	"scripts": {
		"clean": "rimraf lib dist",
		"start": "nodemon",
		"build": "tsc -b .",
		"bundle": "rollup -c rollup.config.js",
		"writeDeployPackageJson": "node writeDeployPackageJson",
		"copyConfiguration": "shx cp -r config/ dist/",
		"copyPublic": "shx cp -r public/ dist/",
		"predeploy:bot": "run-s writeDeployPackageJson copyConfiguration copyPublic"
	},
	"devDependencies": {
		"@tsconfig/node12": "^1.0.7",
		"@types/express": "^4.17.11",
		"@types/node": "^14.14.31",
		"@types/rimraf": "^3",
		"nodemon": "^2.0.7",
		"rimraf": "^3.0.2",
		"shx": "^0.3.3",
		"ts-node": "^9.1.1",
		"typescript": "^4.1.5"
	},
	"dependencies": {
		"config": "^3.3.4",
		"cookie-parser": "^1.4.5",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"got": "^11.8.2",
		"jsonwebtoken": "^8.5.1"
	}
}
