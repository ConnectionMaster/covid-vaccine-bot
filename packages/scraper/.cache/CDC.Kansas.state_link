  $(window).load(function () {
    setTimeout(function () {
      try {
        resizeAdjustmentTS();
      } catch (e) {
        console.error(e);
      }
    }, 350);

    $(window).scroll(function () {
      try {
        scrollAdjustmentTS();
      } catch (e) {
        console.error(e);
      }
    });

    var adjustTimeoutTS;
    $(window).resize(function () {
      clearTimeout(adjustTimeoutTS);
      adjustTimeoutTS = setTimeout(function () {
        try {
          resizeAdjustmentTS();
        } catch (e) {
          console.error(e);
        }
      }, 350);
    });

    $.when(window.Pages.angularToolbarComplete).done(function () {
      try {
        resizeAdjustmentTS();
      } catch (e) {
        console.error(e);
      }
    });
  });
</script>
