        var transformQuery = "#js-odx-content__body";
        var $bodyOfContent = $(transformQuery);
        if (authorSelectedDisplayFormat !== "asTyped" || innerCollapsibleSections !== "asTyped") {
          $bodyOfContent.story_transformContent(story_transformContent_options);
    
          var $tableOfContentsContainer = $('.js-structured-story-body .tableOfContents');
          if( typeof $tableOfContentsContainer === 'object' && $tableOfContentsContainer.length === 0) {
            //$('#js-odx-content__body h2:first').addClass('firstHeader');
            $bodyOfContent.find('h2').first().addClass('firstHeader');
          }
    
          if(authorSelectedDisplayFormat === 'twisties') {
            $bodyOfContent.find('div.sectionHeader').first().trigger('click');
            if(innerCollapsibleSections === 'twisties'){
              $bodyOfContent.find('div.firstsectionHeader').first().trigger('click');
            }
          }
        }

        $('.section.twisty, .firstsection.twisty, .secondLevelSection.twisty').on('keypress',function(event){
            var key = event.which || event.keyCode;
