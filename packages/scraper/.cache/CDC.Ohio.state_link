                var child = jQuery(this).children()[0];
                child.click();
                event.preventDefault();
            }
        });

        var customH2Style = '';
        $bodyOfContent.find('h2').addClass(customH2Style);
    
        
    
        if($('.firstsection.story').hasClass('twisty'))
        {
          $('.firstsection > .firstsectionBody > .secondLevelSection').addClass('nested');
        };
    
      });
    })(jQuery);
    </script>
