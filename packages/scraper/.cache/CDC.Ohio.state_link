        if (innerCollapsibleSections !== "asTyped") {
          story_transformContent_options["sectionClass"] = 'firstLevelSection';
          story_transformContent_options["haveSubSections"] = true;
          story_transformContent_options["sectionClass"] = 'firstsection';
          story_transformContent_options["headerClass"] = 'firstsectionHeader';
          story_transformContent_options["bodyClass"] = 'firstsectionBody';
          story_transformContent_options["subSectionsOptions"] = {
            sectionClass : "secondLevelSection",
