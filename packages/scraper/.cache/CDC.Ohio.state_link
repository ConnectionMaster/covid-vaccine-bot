          var authorSelectedDisplayFormat = "asTyped";
          switch (unprocessedAuthorSelectedDisplayFormat) {
            case '':
              authorSelectedDisplayFormat = "asTyped";
              break;
            case 'As Typed':
              authorSelectedDisplayFormat = "asTyped";
              break;
            case 'Table of Contents':
              authorSelectedDisplayFormat = "toc";
              break;
            case 'Collapsible Sections':
              authorSelectedDisplayFormat = "twisties";
              break;
            case 'Horizontal Tabs':
              authorSelectedDisplayFormat = "horizontalTabs";
              break;
            case 'Vertical Tabs':
              authorSelectedDisplayFormat = "verticalTabs";
              break;
            case 'Horizontal Tabs with Collapsible Sections':
              authorSelectedDisplayFormat = "horizontalTabsWithCollapsibleSections";
              break;
          }
          return authorSelectedDisplayFormat;
        }
        var authorSelectedDisplayFormat = selectDisplayFormat('Horizontal Tabs'),
          innerCollapsibleSections    = selectDisplayFormat('Collapsible Sections');
        var story_transformContent_options = {
          type            : authorSelectedDisplayFormat,
