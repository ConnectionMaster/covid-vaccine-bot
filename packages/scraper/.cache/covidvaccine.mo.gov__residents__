<script type="text/javascript" src="//s7.addthis.com/js/300/addthis_widget.js#pubid=ra-5ff5ecc5a8a60bbc"></script>


<script>


$(document).ready(function(){

  $('#availability_t').click(function(){
    var availability_translate
    var availability_translate_mobile
    var availability_new_path
    var availability_new_path_mobile

    var link
    var view

    availability_translate=$(this).parent().parent().find('img').eq(0).attr('src');
    availability_translate_mobile =$(this).parent().parent().find('img').eq(1).attr('src');


    link = availability_translate.substr(0,6);


  if(link == "/img/v"){
    availability_new_path = "/img/translate/"+ availability_translate.substr(5);
    availability_new_path_mobile = "/img/translate/"+ availability_translate_mobile.substr(5);
    view = "View in English";
  }else if(link == "/img/t"){
    availability_new_path = "/img/"+ availability_translate.substr(15);
    availability_new_path_mobile = "/img/"+ availability_translate_mobile.substr(15);
    view = "Ver imagen en EspaÃ±ol";
  }
  $(this).parent().parent().find('img').eq(0).attr('src',availability_new_path);
  $(this).parent().parent().find('img').eq(1).attr('src',availability_new_path_mobile);
  $(this).text(view);

  });
});


</script>
